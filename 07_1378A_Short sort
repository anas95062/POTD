#include<stdio.h>
int main(){
char str[4] = "abc";
    int i;
    scanf("%d",&i);
    while(i--){
        char s[4];
        scanf("%s",s);
        int flag = 0;
        for(int j=0;j<3;j++){
            if(s[j]==str[j]){
                flag = 1;
                break;
            }
        }
        if(flag) printf("yes\n");
        else printf("no\n");
    }
return 0;
}
