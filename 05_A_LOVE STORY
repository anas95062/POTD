#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    char str[10] = "codeforces";
    char tc[11];
    for(int i=1;i<=n;i++){
        scanf("%s",&tc);
        int count = 0;
        for(int j=0;j<10;j++){
            if(str[j]!=tc[j]){
                count++;
            }
        }
        printf("%d\n",count);
    }
    return 0;
}
